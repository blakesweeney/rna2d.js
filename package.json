{
  "name": "rna2d",
  "version": "0.1.0",
  "description": "A tool for drawing RNA 2D structures",
  "homepage": "https://github.com/blakesweeney/rna2d.js",
  "keywords": [
    "rna",
    "visualization",
    "svg"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blakesweeney/rna2d.js"
  },
  "scripts": {
    "build": "./node_modules/.bin/browserify ./src/main.js -t babelify --standalone Rna2D --outfile rna2d.js",
    "min": "./node_modules/.bin/uglifyjs rna2d.js > rna2d.min.js",
    "test": "./node_modules/.bin/mocha --compilers js:babel/register test/rna2d/*",
    "rel": "npm run build && npm run min"
  },
  "bugs": "https://github.com/blakesweeney/rna2d.js/issues",
  "dependencies": {
    "d3": "~3.5.6"
  },
  "license": {
    "license": "BSD-2-Clause"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babelify": "^6.1.2",
    "chai": "~1.9.1",
    "jscs": "^1.13.1",
    "jsdom": "3",
    "mocha": "^2.2.5",
    "uglify-js": "~2.4.15 "
  },
  "author": {
    "name": "Blake A. Sweeney",
    "email": "bsween@bgsu.edu"
  }
}
